  "launchMode": "focus",
  "minimizeToNotificationArea": false,
  "newTabMenu": [
    {
      "type": "remainingProfiles"
    }
  ],
  "newTabPosition": "afterCurrentTab",
  "profiles": {
    "defaults": {
      "backgroundImageOpacity": 0,
      "bellStyle": "none",
      "colorScheme": "One Half Dark",
      "cursorShape": "filledBox",
      "elevate": false,
      "experimental.retroTerminalEffect": false,
      "font": {
        "face": "Mononoki Nerd Font",
        "weight": "thin"
      },
      "historySize": 10000,
      "icon": "none",
      "intenseTextStyle": "all",
      "opacity": 100,
      "padding": "3",
      "pathTranslationStyle": "wsl",
      "rightClickContextMenu": false,
      "scrollbarState": "hidden",
      "startingDirectory": null,
      "suppressApplicationTitle": true,
      "useAcrylic": false
    },
    "list": [
      {
        "font": {
          "face": "Mononoki Nerd Font"
        },
        "guid": "profile-id",
        "hidden": false,
        "icon": "\ud83d\udd25",
        "name": "Ubuntu",
        "source": "Microsoft.WSL"
      }
    ]
  },
  "schemes": [
    {
      "background": "#300A24",
      "black": "#171421",
      "blue": "#0037DA",
      "brightBlack": "#767676",
      "brightBlue": "#08458F",
      "brightCyan": "#2C9FB3",
